     padding:  4px 0;
     color:  #a0aec0;
     
}

  .mini-table td  {
     padding:  4px 0;
     color:  #4a5568;
     
}

    /* Responsive adjustments */  @media (max-width: 1200px)  {
          .tables-row  {
         grid-template-columns:  1fr;
         
    }

          .bottom-panel  {
         flex-direction:  column;
         
    }

      
}

