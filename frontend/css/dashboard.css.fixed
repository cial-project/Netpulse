    /* Tables Row */  .tables-row  {
          display:  grid;
          grid-template-columns:  1fr 1fr;
          gap:  2rem;
          margin-bottom:  2rem;
      
}

    .device-table-card  {
          background:  white;
          border-radius:  10px;
          box-shadow:  var(--box-shadow);
          padding:  0;
          overflow:  hidden;
      
}

    .card-header  {
          background:  #f7fafc;
          padding:  1rem 1.5rem;
          font-size:  0.75rem;
          font-weight:  700;
          color:  #4a5568;
          border-bottom:  1px solid #edf2f7;
          text-transform:  uppercase;
      
}

    .clean-table  {
          width:  100%;
          border-collapse:  collapse;
      
}

    .clean-table th  {
          text-align:  left;
          padding:  0.75rem 1.5rem;
          font-size:  0.75rem;
          color:  #718096;
          font-weight:  600;
          border-bottom:  1px solid #edf2f7;
      
}

    .clean-table td  {
          padding:  0.75rem 1.5rem;
          font-size:  0.85rem;
          color:  #2d3748;
          border-bottom:  1px solid #edf2f7;
      
}

    .clean-table tr:last-child td  {
     border-bottom:  none;
     
}

  .clean-table tr:hover  {
     background-color:  #f7fafc;
     
}

    .text-green  {
     color:  #38a169;
     
}

  .text-red  {
     color:  #e53e3e;
     
}

  .font-bold  {
     font-weight:  600;
     
}

    /* Bottom Panel */  .bottom-panel  {
          display:  flex;
          gap:  2rem;
          margin-bottom:  3rem;
      
}

    .bottom-panel .device-table-card.grow  {
     flex:  1.2;
     
}

  .bottom-panel .chart-container-large  {
     flex:  0.8;
     
}

    .card-header-flex  {
          background:  #f7fafc;
          padding:  0.75rem 1.5rem;
          font-size:  0.75rem;
          font-weight:  700;
          color:  #4a5568;
          border-bottom:  1px solid #edf2f7;
          display:  flex;
          justify-content:  space-between;
          align-items:  center;
      
}

    /* Toggle Switch */  .toggle-switch  {
          display:  flex;
          align-items:  center;
          gap:  8px;
          font-weight:  normal;
          color:  #718096;
      
}

    .switch  {
          position:  relative;
          display:  inline-block;
          width:  32px;
          height:  18px;
      
}

  .switch input  {
     opacity:  0;
     width:  0;
     height:  0;
     
}

  .slider  {
          position:  absolute;
          cursor:  pointer;
          top:  0;
     left:  0;
     right:  0;
     bottom:  0;
          background-color:  #cbd5e0;
          transition:  .4s;
          border-radius:  34px;
      
}

  .slider:before  {
          position:  absolute;
          content:  "" ;
          height:  14px;
          width:  14px;
          left:  2px;
          bottom:  2px;
          background-color:  white;
          transition:  .4s;
          border-radius:  50%;
      
}

  input:checked + .slider  {
     background-color:  #48bb78;
     
}

  input:checked + .slider:before  {
     transform:  translateX(14px);
     
}

    /* Chart Container Side */  .chart-container-large  {
          background:  white;
          border-radius:  10px;
          box-shadow:  var(--box-shadow);
          padding:  1.5rem;
          display:  flex;
          flex-direction:  column;
      
}

    .chart-header  {
          display:  flex;
          justify-content:  space-between;
          align-items:  center;
          margin-bottom:  1rem;
          font-size:  0.8rem;
          color:  #718096;
      
}

    .chart-legend  {
          display:  flex;
          gap:  1rem;
      
}

    .dot  {
          display:  inline-block;
          width:  8px;
          height:  8px;
          border-radius:  50%;
          margin-right:  4px;
      
}

  .dot.green  {
     background-color:  #48bb78;
     
}

  .dot.blue  {
     background-color:  #4299e1;
     
}

    .mini-table  {
          width:  100%;
          margin-top:  1rem;
          font-size:  0.75rem;
      
}

  .mini-table th  {
